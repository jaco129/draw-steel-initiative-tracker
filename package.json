		"autoprefixer": "^10.4.0",
		"gh-pages": "^6.3.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.0.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"uuid": "^11.1.0"
	}
}
